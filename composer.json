{
    "name": "versatilelibs/laravel-query-builder",
    "description": "Easily build Eloquent queries from API requests",
    "license": "MIT",
    "authors": [
        {
            "name": "Fábio Assunção",
            "email": "fabio23gt@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "laravel/framework": "~5.5.0|~5.6.0|~5.7.0"
    },
    "autoload": {
        "psr-4": {
            "Versatile\\QueryBuilder\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Versatile\\QueryBuilder\\QueryBuilderServiceProvider"
            ]
        }
    }
}
